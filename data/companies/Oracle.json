{
  "company": "Oracle",
  "link": "https://eeho.fa.us2.oraclecloud.com/hcmUI/CandidateExperience/en/sites/CX_1/requisitions?lastSelectedFacet=AttributeChar4&selectedFlexFieldsFacets=%22AttributeChar4%7CStudent%2FIntern%22",
  "scrape": [
    {
      "action": "click",
      "xpath": "//div[@class='search-pagination']"
    },
    {
      "action": "scrape",
      "properties": [
        {
          "name": "title",
          "xpath": "//span[@class='job-tile__title']"
        },
        {
          "name": "location",
          "xpath": "//span[@data-bind='html: primaryLocation']"
        },
        {
          "name": "posting_link",
          "xpath": "//a[@class='job-grid-item__link']",
          "html_property": "href",
          "unique": true
        }
      ]
    },
    {
      "action": "scrape",
      "link_property": "posting_link",
      "properties": [
        {
          "name": "description",
          "xpath": "//div[contains(@class,'job-details__description-content')]"
        },
        {
          "name": "tags",
          "xpath": "//div[contains(@class,'job-details__description-content')]",
          "data_processing": "get_keywords(text)"
        },
        {
          "name": "company_job_id",
          "xpath": "//span[contains(@class,'job-meta__title') and . = 'Job Identification']//..//span[contains(@class,'job-meta__subitem')]"
        },
        {
          "name": "apply_link",
          "xpath": "//button[contains(@class,'apply-now-button')]",
          "html_property": "href"
        }
      ]
    }
  ]
}
